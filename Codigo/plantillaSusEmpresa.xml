                <nombre> $nombrePuntoServicio </nombre>
                <direccion> $direccionPuntoServicio </direccion>
                <listaEscritorios>
                    <escritorio id="$codigoEscritorio">
                        <identificacion> $identificacionEscritorio </identificacion>
                        <encargado> $nombreEncargado </encargado>
                    </escritorio>
                </listaEscritorios>
            </puntoAtencion>
        </listaPuntosAtencion>
        <listaTransacciones>
